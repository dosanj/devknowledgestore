{
  "name": "linkstore",
  "version": "1.0.0",
  "description": "Dev Knowledge Store",
  "private": "true",
  "scripts": {
    "deploy": "npm run build && firebase deploy",
    "build": "cpx src/index.html dist && webpack --mode production",
    "start:prod": "npm run build && web-dev-server --node-resolve --app-index index.html --root-dir dist --open",
    "start": "cpx src/index.html dist && webpack serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dosanj/linkstore.git"
  },
  "author": "Javed Dosani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dosanj/linkstore/issues"
  },
  "homepage": "https://github.com/dosanj/linkstore#readme",
  "dependencies": {
    "lit-html": "^1.3.0"
  },
  "devDependencies": {
    "sass-loader": "^10.1.0",
    "sass": "^1.29.0",
    "@web/dev-server": "0.0.24",
    "cpx": "^1.5.0",
    "css-loader": "^5.0.1",
    "raw-loader": "^4.0.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
